{
  "name": "ows/composer-dependencies-security-checker",
  "type": "library",
  "description": "Security checker for your composer dependencies",
  "keywords": ["security", "dependencies", "checker"],
  "license": "GPL-2.0-or-later",
  "require": {
      "composer/semver": "^1.5 || ^3.0",
      "ext-json": "*"
  },
  "autoload": {
      "psr-4": {
          "Ows\\ComposerDependenciesSecurityChecker\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Ows\\ComposerDependenciesSecurityChecker\\Tests\\": "tests/src"
      }
  },
  "require-dev": {
      "phpunit/phpunit": "^8.5",
      "phpstan/phpstan": "^0.12.5"
  }
}
